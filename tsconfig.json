{
    "compilerOptions": {
        "module": "commonjs",
        "types": [],
        "typeRoots": [
            "node_modules/@types"
        ],
        "target": "es6",
        "lib": [
            "esnext.asynciterable",
            "es6",
            "es2015.promise",
