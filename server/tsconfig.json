{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "../dist/",
    "types": [],
    "typeRoots": [
      "node_modules/@types"
    ],
    "target": "es6",
    "lib": [
			"dom",
      "esnext.asynciterable",
      "es6",
      "es2015.promise"
    ],
    "sourceMap": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "plugins": [],
    "moduleResolution": "node",
  }
}
